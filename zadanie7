using System;


class Program
{
    static void Main()
    {
        int s = 0;

        for (int i = 1; i <= 11; i++)
        {
            Console.WriteLine("podaj a");
            int a = int.Parse(Console.ReadLine());

            if (i > 10)
            {
                Console.WriteLine("suma liczb wiekszych od 5 to  " + s);

            }
            else if (a > 5)
            {
                s = s + a;
            }
        }

    }



}


